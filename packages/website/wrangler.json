{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "vrly-de",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-05-30",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "vrly.de",
      "custom_domain": true,
    },
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
  "kv_namespaces": [
    {
      "binding": "SESSION",
      "id": "7974aa3451be4ffd98aba571f384b058",
    },
  ],
}
