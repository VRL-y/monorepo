{
  "name": "vrly-de",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && bun run cf-typegen && astro build",
    "preview": "bun run build && wrangler dev",
    "astro": "astro",
    "deploy": "bun run build && bun x wrangler deploy",
    "cf-typegen": "wrangler types && prettier --write worker-configuration.d.ts",
    "format": "prettier --write .",
    "check": "astro check",
    "cleanup": "rm -rf .astro .wrangler dist node_modules"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "12.5.4",
    "@astrojs/mdx": "4.3.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "4.0.12",
    "@astrojs/sitemap": "3.4.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.7",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "astro": "5.9.1",
    "astro-webmanifest": "^1.0.0",
    "fuse.js": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-scrollbar-hide": "^4.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "wrangler": "^4.19.1"
  }
}
