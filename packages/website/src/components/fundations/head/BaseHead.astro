---
import Seo from "@/components/fundations/head/Seo.astro";
import Meta from "@/components/fundations/head/Meta.astro";
import Fonts from "@/components/fundations/head/Fonts.astro";

import Fuse from "@/components/scripts/Fuse.astro";
import AOSHead from "@/components/fundations/scripts/AOSHead.astro";
import KeenSlider from "@/components/fundations/scripts/KeenSlider.astro";

// SEO Props that can be passed from pages
export interface Props {
  title?: string;
  description?: string;
  canonical?: string;
  image?: string;
  noindex?: boolean;
  nofollow?: boolean;
  openGraph?: {
    title?: string;
    description?: string;
    image?: string;
    type?: string;
  };
  twitter?: {
    title?: string;
    description?: string;
    image?: string;
  };
  keywords?: string[];
  jsonLd?: object | object[];
}

const seoProps = Astro.props;
---

<Meta />
<Seo {...seoProps} />
<Fonts />

<AOSHead/>
<Fuse />
<KeenSlider />
